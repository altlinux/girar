GEARS_DIR='@GEARS_DIR@'
PEOPLE_DIR='@PEOPLE_DIR@'
HOOKS_DIR='@HOOKS_DIR@'
CONF_DIR='@CONF_DIR@'
REPO_CONF_DIR='@REPO_CONF_DIR@'
REPO_LIST='@REPO_LIST@'
RUNTIME_DIR='@RUNTIME_DIR@'
STATE_DIR='@STATE_DIR@'
GIT_TEMPLATE_DIR='@GIT_TEMPLATE_DIR@'
GITWEB_URL='@GITWEB_URL@'
PACKAGES_EMAIL='@PACKAGES_EMAIL@'
USER_PREFIX='@USER_PREFIX@'

if [ -s "$REPO_CONF_DIR/.default" ]; then
	. "$REPO_CONF_DIR/.default"
fi
if [ -n "${GIRAR_REPO_NAME-}" -a \
     -s "$REPO_CONF_DIR/${GIRAR_REPO_NAME-}" ]; then
	. "$REPO_CONF_DIR/${GIRAR_REPO_NAME-}"
fi

fi #__included_girar_sh_config
